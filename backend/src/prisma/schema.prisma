generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Appointment {
  id        Int      @id @default(autoincrement())
  startDate DateTime
  endDate   DateTime
  paymentId Int
  payment   Payment  @relation(fields: [paymentId], references: [id])
}

model Payment {
  id          Int           @id @default(autoincrement())
  price       Int
  email       String
  appointment Appointment[]
}
